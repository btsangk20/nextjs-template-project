{
  "name": "nextjs-template-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn lint-fix && next dev -p 3000",
    "start": "yarn lint-fix && next start -p 3000",
    "build": "yarn lint-fix && next build",
    "build:stag": "yarn lint-fix && next build",
    "build:prod": "yarn lint-fix && next build",
    "lint": "next lint",
    "postinstall": "husky install",
    "lint-fix": "next lint --dir . --fix"
  },
  "dependencies": {
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}